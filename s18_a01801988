items = ["milk", "eggs", "bread"]
bought = [False, True, False]
print("To buy:", [items[i] for i,b in enumerate(bought) if not b])
for i in range(len(items)):
    if not bought[i]:
        if input(f"Did you buy {items[i]}? (y/n): ").strip().lower().startswith("y"):
            bought[i] = True
print("Updated:", list(zip(items, bought)))
print()


nums = [42, 7, 19, 100, 3, 58]
print("Numbers:", nums)
print("Max:", max(nums))
print("Min:", min(nums))
print("Sorted:", sorted(nums))
print()


nums2 = [1,2,3,4,10,15,22]
evens = [x for x in nums2 if x%2==0]
odds = [x for x in nums2 if x%2!=0]
print("Evens:", evens)
print("Odds:", odds)



users = ["admin", "guest"]
print("Dups:", [u for u in users if users.count(u)>1])
new_user = "user3"
if new_user not in users:
    users.append(new_user)
print("Users:", users)
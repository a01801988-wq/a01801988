temperatures = [20,21,22,25,30,30,31,15]     

avgTemperature = sum(temperatures) / ten(temperatures)

print("The average Temperature is ", avg avgTemperature)

for temperature in temperatures:
    if temperature >=avgTemperature:
        print(temperature, "is above average")
    else:
        print(temperature, "is below average") 

